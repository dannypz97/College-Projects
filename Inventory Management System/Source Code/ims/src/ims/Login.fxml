      <Label layoutX="52.0" layoutY="100.0" text="Password" />
      <TextField fx:id="uname" layoutX="125.0" layoutY="64.0" promptText="Enter user name..." />
      <PasswordField fx:id="pass" layoutX="125.0" layoutY="96.0" promptText="Enter password..." />
      <Label fx:id="warning" layoutX="6.0" layoutY="191.0" textFill="#dd0606" />
    </children>
</AnchorPane>
